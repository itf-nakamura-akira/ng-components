{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "sake-components": {
            "projectType": "library",
            "root": "projects/sake-components",
            "sourceRoot": "projects/sake-components/src",
            "prefix": "lib",
            "architect": {
                "build": {
                    "builder": "@angular/build:ng-packagr",
                    "configurations": {
                        "production": {
                            "tsConfig": "projects/sake-components/tsconfig.lib.prod.json"
                        },
                        "development": {
                            "tsConfig": "projects/sake-components/tsconfig.lib.json"
                        }
                    },
                    "defaultConfiguration": "production"
                },
                "test": {
                    "builder": "@angular/build:unit-test",
                    "options": {
                        "runner": "vitest",
                        "tsConfig": "projects/sake-components/tsconfig.spec.json",
                        "buildTarget": "::development"
                    }
                },
                "lint": {
                    "builder": "@angular-eslint/builder:lint",
                    "options": {
                        "lintFilePatterns": ["projects/sake-components/**/*.ts", "projects/sake-components/**/*.html"],
                        "eslintConfig": "projects/sake-components/eslint.config.js"
                    }
                }
            }
        },
        "sake-components-docs": {
            "projectType": "application",
            "schematics": {
                "@schematics/angular:component": {
                    "style": "scss"
                }
            },
            "root": "projects/sake-components-docs",
            "sourceRoot": "projects/sake-components-docs/src",
            "prefix": "app",
            "architect": {
                "build": {
                    "builder": "@angular/build:application",
                    "options": {
                        "outputPath": "dist/sake-components-docs",
                        "index": "projects/sake-components-docs/src/index.html",
                        "browser": "projects/sake-components-docs/src/main.ts",
                        "polyfills": [],
                        "tsConfig": "projects/sake-components-docs/tsconfig.app.json",
                        "inlineStyleLanguage": "scss",
                        "assets": [
                            {
                                "glob": "**/*",
                                "input": "projects/sake-components-docs/public"
                            }
                        ],
                        "styles": ["projects/sake-components-docs/src/styles.scss"],
                        "scripts": []
                    },
                    "configurations": {
                        "production": {
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "500kB",
                                    "maximumError": "1MB"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "4kB",
                                    "maximumError": "8kB"
                                }
                            ],
                            "outputHashing": "all"
                        },
                        "development": {
                            "optimization": false,
                            "extractLicenses": false,
                            "sourceMap": true
                        }
                    },
                    "defaultConfiguration": "production"
                },
                "serve": {
                    "builder": "@angular/build:dev-server",
                    "configurations": {
                        "production": {
                            "buildTarget": "sake-components-docs:build:production"
                        },
                        "development": {
                            "buildTarget": "sake-components-docs:build:development"
                        }
                    },
                    "defaultConfiguration": "development"
                },
                "extract-i18n": {
                    "builder": "@angular/build:extract-i18n"
                },
                "test": {
                    "builder": "@angular/build:unit-test",
                    "options": {
                        "runner": "vitest",
                        "tsConfig": "projects/sake-components-docs/tsconfig.spec.json",
                        "buildTarget": "::development"
                    }
                },
                "lint": {
                    "builder": "@angular-eslint/builder:lint",
                    "options": {
                        "lintFilePatterns": [
                            "projects/sake-components-docs/**/*.ts",
                            "projects/sake-components-docs/**/*.html"
                        ],
                        "eslintConfig": "projects/sake-components-docs/eslint.config.js"
                    }
                }
            }
        }
    },
    "cli": {
        "analytics": "48dd49ac-1294-4aed-83c4-41b35325eb5e"
    }
}
